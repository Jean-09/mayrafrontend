{
  "kind": "collectionType",
  "collectionName": "inventarios",
  "info": {
    "singularName": "inventario",
    "pluralName": "inventarios",
    "displayName": "inventario"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "sucursal": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::sucursal.sucursal",
      "inversedBy": "inventarios"
    },
    "producto": {
      "type": "string",
      "required": true
    },
    "cantidad": {
      "type": "integer"
    },
    "tipo_movimiento": {
      "type": "enumeration",
      "default": "Recién agregado",
      "enum": [
        "Entrada",
        "Salida",
        "Recién agregado"
      ]
    },
    "fecha": {
      "type": "datetime"
    },
    "observaciones": {
      "type": "text"
    },
    "precio_unitario": {
      "type": "decimal"
    }
  }
}
