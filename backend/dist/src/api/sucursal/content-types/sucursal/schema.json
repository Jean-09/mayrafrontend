{
    "kind": "collectionType",
    "collectionName": "sucursales",
    "info": {
        "singularName": "sucursal",
        "pluralName": "sucursales",
        "displayName": "sucursal"
    },
    "options": {
        "draftAndPublish": true
    },
    "pluginOptions": {},
    "attributes": {
        "users": {
            "type": "relation",
            "relation": "oneToMany",
            "target": "plugin::users-permissions.user",
            "mappedBy": "sucursal"
        },
        "nombre": {
            "type": "string",
            "required": true,
            "unique": true
        },
        "direccion": {
            "type": "text",
            "required": true
        },
        "telefono": {
            "type": "string"
        },
        "activa": {
            "type": "boolean",
            "default": true
        },
        "fecha_creacion": {
            "type": "datetime"
        },
        "pedidos": {
            "type": "relation",
            "relation": "oneToMany",
            "target": "api::pedido.pedido",
            "mappedBy": "sucursal"
        },
        "inventarios": {
            "type": "relation",
            "relation": "oneToMany",
            "target": "api::inventario.inventario",
            "mappedBy": "sucursal"
        }
    }
}
